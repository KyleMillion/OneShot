{
  "name": "@patriot-rentals/report",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "nodemon --watch src --ext ts --exec \"pnpm build && node dist/index.js\"",
    "start": "ts-node src/index.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo reports"
  },
  "dependencies": {
    "pg": "^8.11.5",
    "dotenv": "^16.4.5",
    "handlebars": "^4.7.8",
    "marked": "^12.0.1",
    "fs-extra": "^11.2.0",
    "commander": "^12.0.0",
    "axios": "^1.6.8",
    "child_process": "npm:@types/node",
    "@patriot-rentals/shared-types": "workspace:*",
    "@patriot-rentals/crawler": "workspace:*",
    "@patriot-rentals/scraper": "workspace:*",
    "@temporalio/client": "^1.9.0"
  },
  "devDependencies": {
    "typescript": "~5.4.5",
    "ts-node": "~10.9.2",
    "nodemon": "~3.1.0",
    "@types/pg": "^8.11.5",
    "@types/handlebars": "^4.1.0",
    "@types/marked": "^6.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^20.12.0"
  }
} 